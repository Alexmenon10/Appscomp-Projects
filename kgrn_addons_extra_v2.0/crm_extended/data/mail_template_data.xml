        <!--Mail Template for Service Engagement Confirm-->
        <record id="service_engagemnet_confirmation_maiiissieitlwii" model="mail.template">
            <field name="name">Service Engagement - Send by Email</field>
            <field name="model_id" ref="crm_extended.model_sale_order"/>
            <field name="subject">Service Engagement</field>
            <field name="report_template" ref="sale.action_report_saleorder"/>
