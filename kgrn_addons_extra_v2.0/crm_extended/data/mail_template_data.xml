        <!--Mail Template for Advance Payment Notification-->
            <record id="aassasa" model="mail.template">
            <field name="name">Service Engagement Advance Payment Notification - Send by Email</field>
            <field name="model_id" ref="crm_extended.model_sale_order"/>
            <field name="subject">Service Engagement Advance Payment Notification:</field>
            <field name="email_from">{{object.user_id.email}}</field>
            <field name="email_to"></field>
            <field name="auto_delete" eval="False"/>
            <field name="body_html" type="html">
